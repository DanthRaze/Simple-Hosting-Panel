{
  "name": "Simple Hosting Panel",
  "version": "1.1.0",
  "description": "A professional Simple Hosting Panel for Minecraft servers.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [
    "minecraft",
    "server",
    "panel",
    "hosting"
  ],
  "author": "Theo Tech",
  "license": "ISC",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "adm-zip": "^0.5.10",
    "archiver": "^7.0.1",
    "axios": "^1.13.5",
    "chart.js": "^4.4.1",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "electron-squirrel-startup": "^1.0.1",
    "fix-path": "^3.0.0",
    "pidusage": "^3.0.2",
    "rcon-client": "^4.2.3",
    "sharp": "^0.34.5",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "electron": "^28.3.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": {
          "unpack": "**/node_modules/{sharp,@img}/**/*"
        },
        "icon": "./icon",
        "extraResources": [
          {
            "from": "node_modules/@img/sharp-libvips-darwin-x64/lib/",
            "to": "libvips/"
          }
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-auto-unpack-natives",
          "config": {}
        }
      ]
    }
  }
}
